impoprt numpy as np
x = np.arange(0,100)
y = x*2
z = x**2

import matplotlib.pyplot as plt
#if using juypter notebook
# %matplotlib inline 


#if not using juypter notebook (prints plot)
plt.show() 

#FUNCTIONAL
plt.plot(x,y)

#OO
fig = plt.figure()
